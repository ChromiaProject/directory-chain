<deploy>
    <chain name="manager" container="system">
        <gtv path="blockstrategy/maxblocktime"><int>2000</int></gtv>
        <app module="devnet">
            <args module="directory1">
                <arg key="initial_provider">
                    <bytea>03ECD350EEBC617CBBFBEF0A1B7AE553A748021FD65C7C50C5ABB4CA16D4EA5B05</bytea> <!-- Initial system provider (alpha) -->
                </arg>
                <arg key="genesis_node">
                    <array>
                        <bytea>0350fe40766bc0ce8d08b3f5b810e49a8352fdd458606bd5fafe5acdcdc8ff3f57</bytea>
                        <string>{hostName}</string>
                        <int>9870</int>
                        <string>{apiUrl}</string>
                    </array>
                </arg>
            </args>
            <args module="auth_service">
                <arg key="pubkey">
                    <bytea>02B6F2967CF9AFC4D289EF475A2C2DDEC9EAB79AC60C1C99683E3134074619E635</bytea> <!-- Pubkey of auth-server (gamma) -->
                </arg>
            </args>
        </app>
        <gtv path="signers">
            <array>
                <bytea>0350fe40766bc0ce8d08b3f5b810e49a8352fdd458606bd5fafe5acdcdc8ff3f57</bytea> <!-- Initial blockchain signer (N0) -->
            </array>
        </gtv>
    </chain>
</deploy>
