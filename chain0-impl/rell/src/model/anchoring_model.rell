object system_anchoring_chain {
    mutable rid: byte_array = x"";
}
object cluster_anchoring_config {
    mutable raw_config: byte_array = map<text, gtv>().to_gtv().to_bytes();
}

entity cluster_anchoring_chain {
    key blockchain, cluster;
}

