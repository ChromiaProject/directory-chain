    key block_rid: byte_array;
}

// Deprecated
entity anchored_block_signer { key anchored_block, node; }

// Deprecated
struct base_block_header {
    blockchain_rid: byte_array;
    prev_block_rid: byte_array;
    merkle_root_hash: byte_array;
    timestamp: timestamp;
    height: integer;
    dependencies: gtv;
    extra: map<text, gtv>;
}
