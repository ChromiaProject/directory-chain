// Version

enum Codename {
    Delta,
    Sigma,
    Psi,
    Omega
}

struct Version {
    codename: Codename;
    semver: text;
}

namespace version {
    val Delta = Version(Codename.Delta, "0.1.0");
    val Delta_v0_2_0 = Version(Codename.Delta, "0.2.0");
}

query directory_version() = version.Delta_v0_2_0;
