module;

import ^^.*;

operation create_anchor_block(
    blockchain_rid: byte_array,
    block_height: integer,
    timestamp: integer,
    block_rid: byte_array,
    block_header: byte_array,
    witness: byte_array,
    anchoring_tx_op_index: integer
) {
    create anchor_block(
        blockchain_rid = blockchain_rid,
        block_height = block_height,
        timestamp = timestamp,
        block_rid = block_rid,
        block_header = block_header,
        witness = witness,
        anchoring_tx_op_index = anchoring_tx_op_index
    );
}
