function create_evm_asset_link() {
	val evm_asset = create eif.ft4.evm_erc20 (
		network_id = chain_context.args.evm_asset_network_id,
		address = chain_context.args.evm_asset_address,
		name = chain_context.args.evm_asset_name,
		symbol = chain_context.args.evm_asset_symbol,
		decimals = chain_context.args.evm_asset_decimals,
		mass_exit_support = false
	);
	create eif.ft4.token_mapping ( evm_asset, get_asset() );
}
